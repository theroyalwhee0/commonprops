{
  "name": "@theroyalwhee0/commonprops",
  "version": "0.1.0",
  "description": "TypeScript utilities for extracting common properties from sets of types.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [
    "upcast",
    "utility-types",
    "common-properties"
  ],
  "author": "Adam Mill <hismajesty@theroyalwhee.com> (https://www.theroyalwhee.com/)",
  "license": "Apache-2.0",
  "type": "module",
  "packageManager": "npm@11.4.1",
  "engines": {
    "node": ">=22"
  },
  "files": [
    "dist/",
    "src/",
    "README.md",
    "LICENSE.txt",
    "CHANGELOG.md"
  ],
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "chai": "^5.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "markdownlint-cli": "^0.45.0",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3",
    "vite": "^7.1.12",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "scripts": {
    "build": "vite build",
    "clean": "rm -rf dist",
    "lint": "npm run -s lint:ts && npm run -s lint:eslint && npm run -s lint:md && npm run -s lint:docs",
    "lint:fix": "npm run -s lint:eslint:fix && npm run -s lint:md:fix",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint .",
    "lint:eslint:fix": "eslint . --fix",
    "lint:md": "markdownlint $(git ls-files '*.md')",
    "lint:md:fix": "markdownlint --fix $(git ls-files '*.md')",
    "lint:docs": "typedoc --validation --treatWarningsAsErrors --emit none",
    "test": "vitest run",
    "test:quiet": "vitest run --reporter=dot",
    "docs": "typedoc",
    "prepublishOnly": "npm run clean && npm run build"
  }
}
