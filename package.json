{
  "name": "@theroyalwhee0/commonprops",
  "version": "0.1.0",
  "description": "TypeScript utilities for extracting common properties from sets of types.",
  "main": "src/index.ts",
  "keywords": [
    "upcast",
    "utility-types",
    "common-properties"
  ],
  "author": "Adam Mill <hismajesty@theroyalwhee.com> (https://www.theroyalwhee.com/)",
  "license": "Apache-2.0",
  "type": "module",
  "private": true,
  "packageManager": "npm@11.4.1",
  "engines": {
    "node": ">=22"
  },
  "devDependencies": {
    "@types/chai": "^5.2.2",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.35.1",
    "@typescript-eslint/parser": "^8.35.1",
    "chai": "^5.2.0",
    "eslint": "^9.30.1",
    "eslint-plugin-import": "^2.32.0",
    "markdownlint-cli": "^0.45.0",
    "typedoc": "^0.28.7",
    "typescript": "^5.8.3"
  },
  "scripts": {
    "lint": "npm run -s lint:ts && npm run -s lint:eslint && npm run -s lint:md",
    "lint:fix": "npm run -s lint:ts && npm run -s lint:eslint:fix && npm run -s lint:md:fix",
    "lint:ts": "tsc --noEmit",
    "lint:eslint": "eslint 'src/**/*.ts' --max-warnings=20",
    "lint:eslint:fix": "eslint 'src/**/*.ts' --max-warnings=20 --fix",
    "lint:md": "markdownlint $(git ls-files '*.md')",
    "lint:md:fix": "markdownlint --fix $(git ls-files '*.md')",
    "test": "node --disable-warning=ExperimentalWarning --experimental-strip-types --test --test-timeout=3000 'tests/**/*.test.ts'",
    "test:quiet": "node --disable-warning=ExperimentalWarning --experimental-strip-types --test --test-reporter=dot --test-timeout=3000 'tests/**/*.test.ts'",
    "docs": "typedoc"
  }
}
